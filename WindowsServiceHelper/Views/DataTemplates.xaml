<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:views="clr-namespace:ServiceProcess.Helpers.Views"
                    xmlns:viewModels="clr-namespace:ServiceProcess.Helpers.ViewModels">
    
    <DataTemplate DataType="{x:Type viewModels:ServicesControllerViewModel}">
        <views:ServicesControllerView />
    </DataTemplate>
    
    <DataTemplate DataType="{x:Type viewModels:ServiceViewModel}">
        <views:ServiceView />
    </DataTemplate>
    
</ResourceDictionary>